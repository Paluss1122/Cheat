local lsc = game.Workspace:WaitForChild("Vehicles"):WaitForChild(plr.Name)

local function unvisiblecar()
    if player and lsc then
        for _, obj in ipairs(lsc:GetDescendants()) do
            if obj:IsA("MeshPart") or obj:IsA("Part") then
                obj.Transparency = 1
            elseif obj:IsA("SurfaceGui") then
                obj.Enabled = false
            elseif obj:IsA("Decal") then
                obj.Transparency = 1
            end
        end
    else
        game.StarterGui:SetCore("SendNotification", {
            Title = "Error üî¥",
            Text = "Couldn't make your car invisible!",
            Duration = 5
        })
    end
end

local function visiblecar()
    if player and lsc then
        for _, obj in ipairs(lsc:GetDescendants()) do
            if obj.Name ~= "DriveSeat" and obj.Name ~= "RearRightSeat" and obj.Name ~= "RearLeftSeat" and obj.Name ~= "FrontRightSeat" and obj.Name ~= "Mass" and obj.Name ~= "FL" and obj.Name ~= "FR" and obj.Name ~= "RR" and obj.Name ~= "RL" then
                if obj:IsA("MeshPart") or obj:IsA("Part") then
                    obj.Transparency = 0
                elseif obj:IsA("SurfaceGui") then
                    obj.Enabled = true
                elseif obj:IsA("Decal") then
                    obj.Transparency = 0
                end
            end
        end
    else
        game.StarterGui:SetCore("SendNotification", {
            Title = "Error üî¥",
            Text = "Couldn't make your car visible!",
            Duration = 5
        })
    end
end

unvisibleButton.MouseButton1Click:Connect(function()
    if player then
        for _, obj in ipairs(player:GetChildren()) do
            if obj:IsA("BasePart") then
                obj.Transparency = 1
                obj.CanCollide = false
            elseif obj:IsA("Accessory") then
                obj.Handle.Transparency = 1
            end
        end
        
        if player:FindFirstChild("Head") and player.Head:FindFirstChild("face") then
            player.Head.face.Transparency = 1
        end

        game.StarterGui:SetCore("SendNotification", {
            Title = "Invisible Player",
            Text = "Only you can't see you!",
            Duration = 5
        })
    else
        game.StarterGui:SetCore("SendNotification", {
            Title = "Error üî¥",
            Text = "Couldn't find the player!",
            Duration = 5
        })
    end

    unvisiblecar()
end)


visibleButton.MouseButton1Click:Connect(function()
    if player then
        for _, obj in ipairs(player:GetChildren()) do
            if obj:IsA("BasePart") and obj.Name ~= "HumanoidRootPart" then
                obj.Transparency = 0
                obj.CanCollide = true
            elseif obj:IsA("Accessory") then
                obj.Handle.Transparency = 0
            end
        end
        
        if player:FindFirstChild("Head") and player.Head:FindFirstChild("face") then
            player.Head.face.Transparency = 0
        end

        game.StarterGui:SetCore("SendNotification", {
            Title = "Visible Player",
            Text = "You are now visible!",
            Duration = 5
        })
    else
        game.StarterGui:SetCore("SendNotification", {
            Title = "Error üî¥",
            Text = "Couldn't find the player!",
            Duration = 5
        })
    end

    visiblecar()
end)

-- Help:

helpinformation.MouseButton1Click:Connect(function()
    helps = not helps
    game.StarterGui:SetCore("SendNotification", {
        Title = "Succes üü¢",
        Text = "You've successfully turned the information notifications!",
        Duration = 5
    })
    helpinformation.BackgroundColor3 = helps and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 0, 0)
end)

local function helpings()
    local playerli = game.Players.LocalPlayer

    playerli:GetPropertyChangedSignal("Team"):Connect(function()
        local teamName = playerli.Team and playerli.Team.Name or "None"
        game.StarterGui:SetCore("SendNotification", {
            Title = "Team Change üü¢",
            Text = "You have switched to team: " .. teamName,
            Duration = 5
        })
    end)

    local healthCooldown = 0
    local fuelCooldown = 0
    local healCooldown = 0

    while true do
        wait(1)
        
        if helps then
            if playerli.Character and playerli.Character:FindFirstChild("Humanoid") and playerli.Character.Humanoid.Health <= 50 then
                if tick() >= healthCooldown then
                    game.StarterGui:SetCore("SendNotification", {
                        Title = "‚ùó Warning ‚ùó",
                        Text = "Be careful, you are close to dying!",
                        Duration = 5
                    })
                    healthCooldown = tick() + 40
                end
            end
            if vehicle then
                if fuel <= 0.1 and tick() >= fuelCooldown then
                    game.StarterGui:SetCore("SendNotification", {
                        Title = "‚ùó Warning ‚ùó",
                        Text = "Be careful, you are close to running out of fuel!",
                        Duration = 5
                    })
                    fuelCooldown = tick() + 40
                end

                if heal <= 0.3 and tick() >= healCooldown then
                    game.StarterGui:SetCore("SendNotification", {
                        Title = "‚ùó Warning ‚ùó",
                        Text = "Be careful, you are close to damaging your car!",
                        Duration = 5
                    })
                    healCooldown = tick() + 40
                end
            end
        end
    end
end

helpings()
