local vehicle = game.Workspace.Vehicles:FindFirstChild(plr.Name)
local fuel = vehicle:GetAttribute("CurrentFuel") or 1
local heal = vehicle:GetAttribute("CurrentHealth") or 1

local function alwaysworkingcarf()
    if awc then
        
        game.StarterGui:SetCore("SendNotification", {
            Title = "Always Working Car üü¢",
            Text = "Always working car is now true!",
            Duration = 5
        })
    end
    while awc do
        game.Workspace.Vehicles[plr.Name]:SetAttribute("CurrentHealth", 1)
        game.Workspace.Vehicles[plr.Name]:SetAttribute("IsOn", true)
        game.Workspace.Vehicles[plr.Name]:SetAttribute("CurrentFuel", 1)
        game.Workspace.Vehicles[plr.Name]:SetAttribute("BedDown", false)
            
        game.Workspace[plr.Name].Humanoid.Health = game.Workspace[plr.Name].Humanoid.Health + 0.0005
        wait(0.1)
    end
    game.Workspace.Vehicles[plr.Name]:SetAttribute("CurrentHealth", 1)
    game.Workspace.Vehicles[plr.Name]:SetAttribute("IsOn", false)
    game.Workspace.Vehicles[plr.Name]:SetAttribute("CurrentFuel", 1)
    game.Workspace.Vehicles[plr.Name]:SetAttribute("BedDown", false)

    game.StarterGui:SetCore("SendNotification", {
        Title = "Always Working Car üî¥",
        Text = "Always working car is now false!",
        Duration = 5
    })
end

alwaysworkingcar.MouseButton1Click:Connect(function()
    awc = not awc
    if awc then
        spawn(alwaysworkingcarf)
    end
    alwaysworkingcar.BackgroundColor3 = awc and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 0, 0)
end)

if heal <= 0.1 then
    awc = true
    spawn(alwaysworkingcarf)
    alwaysworkingcar.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
else
    alwaysworkingcar.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
end

hasremotecontrole.MouseButton1Click:Connect(function()
    if not hr then
        plr:SetAttribute("HasRemoteControl", true)
        hr = true
        game.StarterGui:SetCore("SendNotification", {
            Title = "Remote Control üü¢",
            Text = "You got the Remote Control!",
            Duration = 5
        })
    else
        plr:SetAttribute("HasRemoteControl", false)
        hr = false
        game.StarterGui:SetCore("SendNotification", {
            Title = "Remote Control üî¥",
            Text = "You lost the Remote Control!",
            Duration = 5
        })
    end
    hasremotecontrole.BackgroundColor3 = hr and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 0, 0)
end)


delobst.MouseButton1Click:Connect(function()
    del = not del

    local Additional = game.Workspace.Roads.Additional

    for _, obj in pairs(Additional:GetDescendants()) do
        if obj:IsA("BasePart") then
            if del then
                obj.CanCollide = false
                obj.Transparency = 1
                delobst.Text = "Add all Road Obstacles"
            else
                obj.CanCollide = true
                obj.Transparency = 0
                delobst.Text = "Delete all Road Obstacles"
            end
        end
    end

    if del then
        game.StarterGui:SetCore("SendNotification", {
            Title = "‚ùóRemoved Road Obstacles‚ùó",
            Text = "All Road Obstacles were removed!",
            Duration = 5
        })
    else
        game.StarterGui:SetCore("SendNotification", {
            Title = "‚ùó Added Road Obstacles ‚ùó",
            Text = "All Road Obstacles were added!",
            Duration = 5
        })
    end
end)

local function findNearestVehicleSeat()
    local nearestSeat = nil
    local shortestDistance = math.huge

    for _, object in ipairs(workspace:GetDescendants()) do
        if object:IsA("VehicleSeat") then
            local distance = (object.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
            if distance < shortestDistance then
                shortestDistance = distance
                nearestSeat = object
            end
        end
    end
    return nearestSeat
end

stealnearestvehicle.MouseButton1Click:Connect(function()
    local character = plr.Character
    local seat = findNearestVehicleSeat()

    if seat then
        character:SetPrimaryPartCFrame(seat.CFrame)
        seat:Sit(character.Humanoid)
    else
        warn("Kein Vehicle Seat in der N√§he gefunden")
    end
end)

